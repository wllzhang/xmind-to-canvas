{
  "name": "@wllzhang/xmind-to-canvas",
  "version": "1.1.6",
  "description": "Convert XMind files to JSON Canvas format",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "rollup --config rollup.config.js -w",
    "build": "rollup --config rollup.config.js --environment BUILD:production",
    "build:web": "rollup --config rollup.config.js --environment BUILD:production && echo 'Web bundle ready at web/xmind-to-canvas.umd.js'",
    "build:npm": "rollup --config rollup.config.js --environment BUILD:production && npm run build:types",
    "build:types": "tsc --project tsconfig.build.json",
    "prepublishOnly": "npm run build:npm",
    "serve:web": "npx serve web -p 3000",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "obsidian",
    "plugin",
    "xmind",
    "canvas",
    "mind-map",
    "json-canvas",
    "converter"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wllzhang/xmind-to-canvas.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@eslint/compat": "^1.3.1",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@typescript-eslint/utils": "^8.33.1",
    "@vitest/coverage-v8": "^2.1.0",
    "builtin-modules": "^3.3.0",
    "eslint": "^9.0.0",
    "eslint-plugin-obsidianmd": "0.1.9",
    "typescript-eslint": "^8.35.1",
    "obsidian": "latest",
    "rollup": "^4.9.6",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vitest": "^2.1.0"
  },
  "dependencies": {
    "@types/jszip": "3.4.0",
    "elkjs": "^0.9.3",
    "jszip": "3.10.1"
  },
  "overrides": {
    "esbuild": "^0.25.0"
  }
}
